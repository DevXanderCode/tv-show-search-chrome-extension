{
  "manifest_version": 3,
  "name": "Tv Show Search",
  "description": "An Extension that helps you search for all your favorite TV shows.",
  "version": "1.0",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_icon": "icon.png",
    "default_title": "TV Shows Search",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
